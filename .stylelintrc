{
    "extends": "stylelint-config-standard",
    "rules": {
        "indentation": 4,
        "unit-whitelist": ["px", "dpi", "dppx", "em", "rem", "s", "%", "deg"],
        "selector-type-no-unknown": false,
        "no-eol-whitespace": false,
        "at-rule-no-unknown": false,
        "at-rule-empty-line-before": false
    }
}
